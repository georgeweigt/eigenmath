
-x
check(mathml(last)=="<math><mrow><mo prefix='true'>-</mo><mi>x</mi></mrow></math>")

x + 1
check(mathml(last)=="<math><mrow><mi>x</mi><mo>+</mo><mn>1</mn></mrow></math>")

x - 1
check(mathml(last)=="<math><mrow><mi>x</mi><mo>-</mo><mn>1</mn></mrow></math>")

-x + 1
check(mathml(last)=="<math><mrow><mo prefix='true'>-</mo><mi>x</mi><mo>+</mo><mn>1</mn></mrow></math>")

1.0
check(mathml(last)=="<math><mrow><mn>1.0</mn></mrow></math>")

-1.0
check(mathml(last)=="<math><mrow><mo prefix='true'>-</mo><mn>1.0</mn></mrow></math>")

1.2
check(mathml(last)=="<math><mrow><mn>1.2</mn></mrow></math>")

-1.2
check(mathml(last)=="<math><mrow><mo prefix='true'>-</mo><mn>1.2</mn></mrow></math>")

1.0 10^6
check(mathml(last)=="<math><mrow><mrow><mn>1.0</mn><mo>&times;</mo><msup><mn>10</mn><mrow><mn>6</mn></mrow></msup></mrow></mrow></math>")

1.0 10^(-6)
check(mathml(last)=="<math><mrow><mrow><mn>1.0</mn><mo>&times;</mo><msup><mn>10</mn><mrow><mo form='prefix'>-</mo><mn>6</mn></mrow></msup></mrow></mrow></math>")

1.2 10^6
check(mathml(last)=="<math><mrow><mrow><mn>1.2</mn><mo>&times;</mo><msup><mn>10</mn><mrow><mn>6</mn></mrow></msup></mrow></mrow></math>")

sqrt(i)
check(mathml(last)=="<math><mrow><msup><mrow><mo>(</mo><mrow><mo prefix='true'>-</mo><mn>1</mn></mrow><mo>)</mo></mrow><mfrac><mn>1</mn><mn>4</mn></mfrac></msup></mrow></math>")

1/sqrt(i)
check(mathml(last)=="<math><mrow><mfrac><mn>1</mn><msup><mrow><mo>(</mo><mrow><mo prefix='true'>-</mo><mn>1</mn></mrow><mo>)</mo></mrow><mfrac><mn>1</mn><mn>4</mn></mfrac></msup></mfrac></mrow></math>")

a/b
check(mathml(last)=="<math><mrow><mfrac><mrow><mi>a</mi></mrow><mrow><mi>b</mi></mrow></mfrac></mrow></math>")

-a/b
check(mathml(last)=="<math><mrow><mo prefix='true'>-</mo><mfrac><mrow><mi>a</mi></mrow><mrow><mi>b</mi></mrow></mfrac></mrow></math>")

x^2
check(mathml(last)=="<math><mrow><msup><mi>x</mi><mrow><mn>2</mn></mrow></msup></mrow></math>")

gmunu
check(mathml(last)=="<math><mrow><msub><mi>g</mi><mrow><mi>&mu;</mi><mi>&nu;</mi></mrow></msub></mrow></math>")

gmunu^2
check(mathml(last)=="<math><mrow><msup><msub><mi>g</mi><mrow><mi>&mu;</mi><mi>&nu;</mi></mrow></msub><mrow><mn>2</mn></mrow></msup></mrow></math>")



