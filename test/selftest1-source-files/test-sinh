"Testing sinh"

clear

check(sinh(0) == 0)
check(sinh(-x) == -sinh(x))
check(sinh(arcsinh(x)) == x)

for(x,-12,12,
  d = float(sinh(x/12)) - sinh(float(x/12)),
  check(abs(d) < 1.0 10^(-6))
)

-- complex arg

for(x,-3,3,for(y,-3,3,
  z = 1/3.0 (x + i y),
  w = 1/2 exp(z) - 1/2 exp(-z),
  check(abs(sinh(z) - w) < 1.0 10^(-6))
))
