     (A51,A52,A53,A54,A55))

for(i,1,5,for(j,1,5,check(det(minormatrix(A,i,j)) == minor(A,i,j))))

A = ((A11,A12,A13,A14,A15,A16),
     (A21,A22,A23,A24,A25,A26),
     (A31,A32,A33,A34,A35,A36),
