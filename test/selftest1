check(infixform(T)="x + 2.71828")
T = float(x + y + 1/4)
check(infixform(T)="x + y + 0.25")
T = float(1/4 x y)
check(infixform(T)="0.25 x y")
T = float(sqrt(2))
check(infixform(T)="1.41421")
