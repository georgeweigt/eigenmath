-- line integral (p. 21)

clear

P=1-y
Q=x
x=2*cos(t)
y=2*sin(t)
defint(P*d(x,t)+Q*d(y,t),t,0,2pi)
check(last=8pi)

-- surface integral (p. 21)

clear

P=1-y
Q=x
h=sqrt(4-x^2)
defint(d(Q,x)-d(P,y),y,-h,h,x,-2,2)
check(last=8pi)

-- surface integral using polar coordinates

f=d(Q,x)-d(P,y) --do before change of coordinates
x=r*cos(theta)
y=r*sin(theta)
defint(f*r,r,0,2,theta,0,2pi)
check(last=8pi)
defint(f*r,theta,0,2pi,r,0,2) --try integrating over theta first
check(last=8pi)

clear

